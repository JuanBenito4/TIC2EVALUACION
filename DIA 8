#include<iostream>
#include<stdio.h>

using namespace std;

int main(){
	char letra[]="historia";
	printf("letra[0]=%c", letra[0]);
	
	return 0;
}


#include<iostream>
#include<stdio.h>

using namespace std;

int main(){
	char letra[]="historia";
	for(int i=0;i<=7;i++){
		printf("Dame la letra[%d]=%c\n",i,letra[i]);
	}
	
	return 0;
}
